{
  "manifest_version": 3,
  "name": "ChatGPT Keyboard Shortcuts",
  "version": "1.0",
  "description": "Adds shortcuts to ChatGPT",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ]
}
